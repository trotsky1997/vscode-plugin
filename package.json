{
    "name": "aixcoder",
    "displayName": "aiXcoder AI Code Completer",
    "description": "The best code completion tool for Java.",
    "version": "0.2.2",
    "publisher": "aixcoder-plugin",
    "repository": {
        "url": "https://github.com/aixcoder-plugin/vscode-plugin/tree/local"
    },
    "bugs": {
        "url": "https://github.com/aixcoder-plugin/vscode-plugin/issues"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:java"
    ],
    "main": "./dist/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "AiXcoder",
            "properties": {
                "aiXcoder.endpoint": {
                    "type": "string",
                    "default": "http://localhost:8787/",
                    "description": "%aiXcoder.configuration.endpoint%"
                },
                "aiXcoder.searchEndpoint": {
                    "type": "string",
                    "default": "https://search.aixcoder.com/",
                    "description": "%aiXcoder.configuration.searchEndpoint%"
                },
                "aiXcoder.searchIntegratedWindow": {
                    "type": "boolean",
                    "default": false,
                    "description": "%aiXcoder.configuration.searchIntegratedWindow%"
                },
                "aiXcoder.searchDefaultBrowser": {
                    "type": "string",
                    "default": "-",
                    "markdownDescription": "%aiXcoder.configuration.searchDefaultBrowser%"
                },
                "aiXcoder.additionalParameters": {
                    "type": "string",
                    "default": "",
                    "description": "%aiXcoder.configuration.additionalParameters%"
                },
                "aiXcoder.symbol": {
                    "type": "string",
                    "default": "‚≠ê",
                    "description": "%aiXcoder.configuration.symbol%"
                }
            }
        },
        "commands": [
            {
                "command": "aiXcoder.resetMessage",
                "title": "%aiXcoder.command.resetMessages.title%",
                "category": "aiXcoder"
            },
            {
                "command": "aiXcoder.search",
                "title": "%aiXcoder.command.search.title%",
                "category": "aiXcoder"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorLangId == python || editorLangId == java || editorLangId == c || editorLangId == cpp",
                    "command": "aiXcoder.search",
                    "group": "4_search"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch-webpack": "webpack --mode none --watch",
        "watch-tsc": "tsc --watch",
        "watch": "npx concurrently 'npm:watch-*'",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/chai": "^4.2.5",
        "@types/download": "^6.2.4",
        "@types/fs-extra": "^8.0.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.12.7",
        "@types/request-promise": "^4.1.44",
        "@types/uuid": "^3.4.6",
        "@types/vscode": "^1.18.0",
        "chai": "^4.2.0",
        "concurrently": "^5.0.0",
        "eslint": "^6.6.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "mocha": "^6.2.2",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.1",
        "typescript": "^3.7.2",
        "vscode-test": "^1.2.3",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "agentkeepalive": "^4.1.0",
        "download": "^7.1.0",
        "find-java-home": "^1.1.0",
        "fs-extra": "^8.1.0",
        "request": "^2.88.0",
        "request-promise": "^4.2.5",
        "uuid": "^3.3.3"
    }
}
