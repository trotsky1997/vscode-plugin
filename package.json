{
    "name": "aixcoder",
    "displayName": "aiXcoder",
    "description": "aiXcoder code suggestion",
    "version": "0.1.12",
    "publisher": "nnthink",
    "repository": {
        "url": "https://github.com/felixhao28/vscode-plugin"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "icon": "images/icon.png",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:python",
        "onLanguage:java",
        "onLanguage:c",
        "onLanguage:cpp",
        "onLanguage:php",
        "onLanguage:typescript",
        "onLanguage:typescriptreact",
        "onLanguage:javascript",
        "onLanguage:javascriptreact",
        "onLanguage:vue"
    ],
    "main": "./dist/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "AiXcoder",
            "properties": {
                "aiXcoder.endpoint": {
                    "type": "string",
                    "default": "",
                    "description": "Specifies the endpoint for code suggestion server."
                },
                "aiXcoder.model.java": {
                    "type": "string",
                    "default": "java(Java)",
                    "markdownDescription": "The model name to be used in Java. Default: `java(Java)`."
                },
                "aiXcoder.model.python": {
                    "type": "string",
                    "default": "python(Python)",
                    "markdownDescription": "The model name to be used in Python. Default: `python(Python)`."
                },
                "aiXcoder.model.cpp": {
                    "type": "string",
                    "default": "cpp(Cpp)",
                    "markdownDescription": "The model name to be used in C++/C. Default: `cpp(Cpp)`."
                },
                "aiXcoder.model.php": {
                    "type": "string",
                    "default": "php(Php)",
                    "markdownDescription": "The model name to be used in Php. Default: `php(Php)`."
                },
                "aiXcoder.model.javascript": {
                    "type": "string",
                    "default": "javascript(JavaScript)",
                    "markdownDescription": "The model name to be used in JavaScript. Default: `javascript(JavaScript)`."
                },
                "aiXcoder.model.typescript": {
                    "type": "string",
                    "default": "typescript(TypeScript)",
                    "markdownDescription": "The model name to be used in TypeScript. Default: `typescript(TypeScript)`."
                },
                "aiXcoder.enableTelemetry": {
                    "type": "boolean",
                    "default": false,
                    "description": "Send anonoymous usage data and errors to aiXcoder telemetry service."
                },
                "aiXcoder.additionalParameters": {
                    "type": "string",
                    "default": "",
                    "description": "(Only for advanced usages) URL encoded params."
                },
                "aiXcoder.alwaysTrigger": {
                    "type": "boolean",
                    "default": true,
                    "description": "If set to true, aiXcoder suggestions are always triggered as you type. Otherwise, it only triggers explictly with e.g. `Ctrl+Space`."
                },
                "aiXcoder.autoImport": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically import missing packages in completions."
                },
                "aiXcoder.sortOnly": {
                    "type": "boolean",
                    "default": false,
                    "description": "No long completion, only sorts the completions (will decrease delay)."
                },
                "aiXcoder.longResultRank": {
                    "type": "integer",
                    "default": 1,
                    "minimum": 1,
                    "maximum": 5,
                    "description": "The prefered rank of long completion result."
                },
                "aiXcoder.longResultCuts": {
                    "type": "string",
                    "default": "Auto",
                    "enum": [
                        "Auto",
                        "0-None",
                        "1",
                        "2",
                        "3",
                        "4",
                        "5"
                    ],
                    "description": "Number of shorter results"
                },
                "aiXcoder.longResultCutSort": {
                    "type": "string",
                    "default": "Long to short",
                    "enum": [
                        "Short to long",
                        "Long to short"
                    ],
                    "description": "The order of shorter results"
                }
            }
        },
        "commands": [{
            "command": "aixcoder.switchModel",
            "title": "Switch Model",
            "category": "aiXcoder"
        }, {
            "command": "aiXcoder.resetMessage",
            "title": "Reset messages",
            "category": "aiXcoder"
        }],
        "javaExtensions": [
            "dist/aixjdtls.jar"
        ]
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "compile": "webpack --mode none",
        "watch": "webpack --mode none --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.7.2",
        "@types/request-promise": "^4.1.44",
        "@types/uuid": "^3.4.5",
        "eslint": "^6.2.0",
        "eslint-config-standard": "^13.0.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^9.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.0",
        "mocha": "^6.2.0",
        "ts-loader": "^6.0.4",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "vscode": "^1.1.36",
        "webpack": "^4.39.2",
        "webpack-cli": "^3.3.7"
    },
    "dependencies": {
        "agentkeepalive": "^4.0.2",
        "request-promise": "^4.2.4",
        "uuid": "^3.3.3"
    }
}
